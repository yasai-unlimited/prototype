$('#starBtn-<%= @family_post.id %>').addClass("disabled");
var speed = 1000;
var unfamilyPostStar = function() {
  var familyPostStarCount = $('#familyPostStarCount-<%= @family_post.id %>');
  $('#familyPostStar-<%= @family_post.id %>').html("<%= escape_javascript(render('family_post_stars/family_post_star', { family_post: @family_post })) %>");
  familyPostStarCount.html(<%= @family_post_stars_count %>);
  $('#starBtn-<%= @family_post.id %>').removeClass("disabled");
};
setTimeout(unfamilyPostStar, speed);